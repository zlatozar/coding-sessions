/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {

    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=nota);
    Nand(a=b, b=b, out=notb);
    
    Nand(a=nota, b=b, out=notab);
    Nand(a=a, b=notb, out=notba);
    Nand(a=notab, b=notba, out=out);
}
