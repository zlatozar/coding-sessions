/** 
 * Multiplexor. If sel == 1 then out = b else out = a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // a and !sel
    Not(in=sel,out=nsel);
    And(a=a,b=nsel,out=ansel);
    // b and sel
    And(a=b,b=sel,out=bsel);
    // (a and !sel) or (b and sel)
    Or(a=ansel,b=bsel,out=out);
}
